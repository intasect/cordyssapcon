function deleteLDAPEntry(index) {

    var request = cordys.cloneXMLDocument(requestDeleteProfile.XMLDocument);

    if (selOrg[index].getFormattedValue() == "system") {
        soapNodeDN = "cn=" + selSN[index].getFormattedValue() + ",cn=soap nodes," + systemDN;
    } else {
        soapNodeDN = "cn=" + selSN[index].getFormattedValue() + ",cn=soap nodes," + userOrganizationDN;
    }
    soapNodeCN = soapNodeDN.split(",")[0];
    userDN = "cn=" + selCUser[index].getFormattedValue() + ",cn=organizational users," + userOrganizationDN;

    var entry = cordys.selectXMLNode(request, ".//*[local-name()='old']/*[local-name()='entry']");

    cordys.setXMLAttribute(entry, "", "dn", soapNodeCN + "," + userDN);

    cordys.setNodeText(entry, ".//*[local-name()='cn']/*[local-name()='string']", soapNodeCN.substring(3));

    cordys.setNodeText(entry, ".//*[local-name()='service']/*[local-name()='string']", soapNodeDN);

    cordys.setNodeText(entry, ".//*[local-name()='busmappedusername']/*[local-name()='string']", txtSUser[index].getValue());

    UpdateOperationModel.setMethodRequest(request);
    UpdateOperationModel.getDataset();
}
