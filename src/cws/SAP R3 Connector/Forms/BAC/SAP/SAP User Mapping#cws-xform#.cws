function getMappedUsersDetails() {
    var userdetails = cordys.cloneXMLDocument(GetOrganizationalUsersoperationModel.getData());
    var soapenv = cordys.cloneXMLDocument(getchildrenreq);
    var soapbody = cordys.selectXMLNode(soapenv, ".//SOAP:Body");

    var entry = cordys.selectXMLNodes(userdetails, ".//tns:entry");
    if (entry.length > 0) {

        cordys.setNodeText(soapbody, ".//tns:dn", entry[0].getAttribute("dn"));

        for (var i = 1; i < entry.length; i++) {
            var getchildrenrequest = (cordys.selectXMLNode(soapbody, ".//tns:GetChildren")).cloneNode(true);
            cordys.setNodeText(getchildrenrequest, ".//tns:dn", entry[i].getAttribute("dn"));
            cordys.appendXMLNode(getchildrenrequest, soapbody);
        }
    }
    GetChildrenOperationModel.setMethodRequest(soapenv);
    GetChildrenOperationModel.getDataset();

    var response = GetChildrenOperationModel.getData();

    var tuples = cordys.selectXMLNodes(response, ".//tns:tuple/tns:old/tns:entry");

    var result = cordys.cloneXMLDocument(xmlID.XMLDocument);
    var getchildrenresponse = cordys.selectXMLNode(result, ".//GetChildrenResponse");
    for (var i = 0; i < tuples.length; i++) {
        var displayEntry = cordys.selectXMLNode(cordys.cloneXMLDocument(entryData.XMLDocument), ".//tuple");

        var service = cordys.getNodeText(tuples[i], ".//tns:service/tns:string");
        cordys.setNodeText(displayEntry, ".//servicegroupDN", service);

        var org = service.substring(service.indexOf("o="));
        cordys.setNodeText(displayEntry, ".//organizationDN", org);

        var userdn = tuples[i].getAttribute("dn").substring(tuples[i].getAttribute("dn").indexOf(",cn=") + 1);
        cordys.setNodeText(displayEntry, ".//cordysuserDN", userdn);

        var sapuser = cordys.getNodeText(tuples[i], ".//tns:busmappedusername/tns:string");
        cordys.setNodeText(displayEntry, ".//sapUserName", sapuser);

        var sappassword = cordys.getNodeText(tuples[i], ".//tns:busmappeduserpassword/tns:string");
        cordys.setNodeText(displayEntry, ".//sapPassword", sappassword);

        cordys.appendXMLNode(displayEntry, getchildrenresponse);
    }
    UserMappingmodel.putData(result, true, "", true);

    initclick = true;
    navInsert1.click();
}

