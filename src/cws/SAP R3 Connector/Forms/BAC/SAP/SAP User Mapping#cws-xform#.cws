function setOrganization() {
    orgUser = new Array();
    sapNodes_org = new Array();
    sapNodes_sys = new Array();
    Flag_SOAPFault = true;

    //Set Current Organization
    userOrganizationDN = application.organization;
    userOrgName = userOrganizationDN.split(",")[0].substring(2);
    var option = {
        value: userOrganizationDN,
        description: userOrgName
    };
    selOrg[0].addOption(option, false, 0, false);

    //Set System Organization
    if (userOrgName != "system") {
        systemDN = "o=system" + "," + userOrganizationDN.substring(userOrganizationDN.indexOf(",") + 1);
        var option = {
            value: systemDN,
            description: "system"
        };
        selOrg[1].addOption(option, false, 0, false);
    }

    //Get All SAP SOAP Processors
    getAllSapNodes();

    //Get Mapped Users
    getMappedUsersDetails();

}

function getAllSapNodes() {
    getSOAPNodesInOrganization(GetSoapProcessorsResponseModel, systemDN, sapNodes_sys);
    model1.putData(cordys.cloneXMLDocument(GetSoapProcessorsResponseModel.getData()), true, null, true);

    if (userOrgName != "system") {
        getSOAPNodesInOrganization(GetSoapProcessorsOperationModel, userOrganizationDN, sapNodes_org);
        var tuples = cordys.selectXMLNodes(cordys.cloneXMLDocument(GetSoapProcessorsOperationModel.getData()), ".//tns:tuple");
        var data = cordys.cloneXMLDocument(GetSoapProcessorsResponseModel.getData());
        for (var i = 0; i < tuples.length; i++)
            cordys.appendXMLNode(tuples[i], cordys.selectXMLNode(data, ".//tns:tuple").parentNode);
        model1.putData(data, true, null, true);

    }
}

function getSOAPNodesInOrganization(Model, organization, soapNodeArray) {
    var request = Model.getMethodRequest();
    cordys.setNodeText(request, ".//*[local-name()='dn']", organization);
    Model.setMethodRequest(request);
    Model.reset();

}

