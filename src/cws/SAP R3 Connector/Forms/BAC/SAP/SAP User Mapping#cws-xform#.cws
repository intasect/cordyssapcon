function selSN_BeforeFill(eventObject) {
    var optionAddFlag = false;
    var config = cordys.getNodeText(cordys.selectXMLNode(eventObject.xml, ".//*[local-name()='bussoapprocessorconfiguration']"), ".//*[local-name()='string']", "<faild/>");
    var confTags = cordys.selectXMLNodes(cordys.loadXMLDocument(config), ".//*[local-name()='configurations']/*[local-name()='configuration']");
    for (var confcount = 0; confcount < confTags.length; confcount++) {
        var impl = confTags[confcount].getAttribute("implementation");;
        if (impl == null)
            continue;
        if (impl.substring(impl.lastIndexOf(".") + 1, impl.length) == "SAPMapper") {
            var dnValue = eventObject.xml.getAttribute("dn");

            eventObject.description = dnValue.split(",")[1].split("=")[1];
            eventObject.inputOption.description = dnValue.split(",")[1].split("=")[1];
            eventObject.inputOption.value = dnValue.substring(dnValue.indexOf(",") + 1);

            cordys.setNodeText(eventObject.xml, ".//*[local-name()='cn']/*[local-name()='string']", dnValue.split(",")[1].split("=")[1]);

            eventObject.xml.setAttribute("dn", dnValue.substring(dnValue.indexOf(",") + 1));

            optionAddFlag = true;
        }
    }
    eventObject.returnValue = optionAddFlag;
}

function selSN_InFocus(eventObject) {
    eventObject.srcElement.removeDuplicates(eventObject.srcElement.getAllOptions());
}

