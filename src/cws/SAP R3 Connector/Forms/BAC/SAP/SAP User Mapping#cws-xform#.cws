function navInsert1_BeforeInsert(eventObject) {

    if (initclick == true) {
        initclick = false;
        return;
    }
    var result = save(eventObject);
    if (result == false) {
        eventObject.returnValue = false;
    } else {
        index = tMappingDetails.getRows().length;
        soapNodeDN = selSN[index].getValue();
        soapNodeCN = soapNodeDN.split(",")[0];
        userDN = selCUser[index].getValue();

        var request = cordys.cloneXMLDocument(requestUpdateProfile.XMLDocument);

        var entry = cordys.selectXMLNode(request, ".//*[local-name()='new']/*[local-name()='entry']");

        cordys.setXMLAttribute(entry, "", "dn", soapNodeCN + "," + userDN);
        cordys.setNodeText(entry, ".//*[local-name()='cn']/*[local-name()='string']", soapNodeCN.substring(3));
        cordys.setNodeText(entry, ".//*[local-name()='service']/*[local-name()='string']", soapNodeDN);
        cordys.setNodeText(entry, ".//*[local-name()='busmappedusername']/*[local-name()='string']", txtSUser[index].getValue());
        cordys.setNodeText(entry, ".//*[local-name()='busmappeduserpassword']/*[local-name()='string']", encode(txtSPassword[index].getValue()));

        UpdateOperationModel.setMethodRequest(request);
        UpdateOperationModel.reset();

        disableRow(tMappingDetails.getIndex());
    }
}

function save(eventObject) {
    var index = tMappingDetails.getIndex();

    if (selOrg[index] == null || selOrg[index].getValue() == "") {
        alert("Select an Organization.");
        selOrg[index].focus();
        return false;
    } else if (selSN[index] == null || selSN[index].getValue() == "") {
        alert("Select a SAP Service Group.");
        selSN[index].focus();
        return false;
    } else if (selCUser[index] == null || selCUser[index].getValue() == "") {
        alert("Select the Cordys User to be mapped.");
        selCUser[index].focus();
        return false;
    } else if (txtSUser[index] == null || !validText(txtSUser[index].getValue())) {
        alert("Please enter the SAP User to which the mapping should be done.");
        txtSUser[index].focus();
        return false;
    } else if (txtSPassword[index] == null || !validText(txtSPassword[index].getValue())) {
        alert("Please enter the user password.");
        txtSPassword[index].focus();
        return false;
    }

    return true;
}

function validText(givenText) {
    if (givenText == "" || !givenText.match(/\S/))
        return false;
    return true;
}

function navInsert1_AfterInsert(eventObject) {
    enableRow(tMappingDetails.getIndex());

}

function encode(str) {
    var result = "";
    //This is the output
    var chr1,
    chr2,
    chr3 = "";
    //These are the 3 bytes to be encoded
    var enc1,
    enc2,
    enc3,
    enc4 = "";
    //These are the 4 encoded bytes
    var i = 0;
    //Position counter
    do {
        //Set up the loop here
        chr1 = str.charCodeAt(i++);
        //Grab the first byte
        chr2 = str.charCodeAt(i++);
        //Grab the second byte
        chr3 = str.charCodeAt(i++);
        //Grab the third byte
        //Here is the actual base64 encode part.
        //There really is only one way to do it.
        enc1 = chr1 >> 2;
        enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
        enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
        enc4 = chr3 & 63;

        if (isNaN(chr2))
            enc3 = enc4 = 64;
        else if (isNaN(chr3))
            enc4 = 64;

        //Lets spit out the 4 encoded bytes
        result = result + keyStr.charAt(enc1) + keyStr.charAt(enc2) + keyStr.charAt(enc3) + keyStr.charAt(enc4);

        // OK, now clean out the variables used.
        chr1 = chr2 = chr3 = "";
        enc1 = enc2 = enc3 = enc4 = "";

    }
    while (i < str.length);
    //And finish off the loop
    //Now return the encoded values.
    return result;
}

